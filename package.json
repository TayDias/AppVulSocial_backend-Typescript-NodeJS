    "node-pre-gyp": "^0.15.0",
    "nodemailer": "^6.4.17",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "socket.io": "^2.4.1"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.0",
    "copyfiles": "^2.3.0",
    "sqlite3": "^5.0.0",
